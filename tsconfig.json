{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "incremental": true,
    "downlevelIteration": true,
    "isolatedModules": true
  },
  "include": ["**/*.ts", "**/*.tsx", "gatsby-node.ts", "gatsby-config.ts", "gatsby-types.d.ts", "graphql.config.js"]
}
